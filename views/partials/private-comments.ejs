<table class="table">
    <tbody>
    <% for (var i = 0; i < privateComments.length; i++) {%>
    <tr>
        <td>
            <small class="text-muted>
            <%= privateComments[i].created.toUTCString()%></small><br/>
            <%= privateComments[i].content %>
        </td>
    </tr>
    <% } %>
    </tbody>
</table>
